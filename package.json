{
  "name": "DSA Board",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --node-env development",
    "dev": "concurrently -n app,lib \"npm start\" \"cd lib && npm start\"",
    "prebuild": "cd lib && npm build",
    "build": "webpack --config webpack.prod.js --node-env production --progress",
    "dev:build": "webpack --config webpack.dev.js --node-env development --progress"
  },
  "devDependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.0",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "cssnano": "^5.0.12",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "monaco-editor": "^0.34.0",
    "raw-loader": "^4.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  }
}
